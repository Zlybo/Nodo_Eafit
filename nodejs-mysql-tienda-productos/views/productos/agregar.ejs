<%- include("../header"); %>
<div class="col-12">
    <h1>Agregar producto</h1>
    <form method="post" action="/productos/insertar">
        <div class="form-group">
            <label for="nombre">Nombre</label>
            <input required id="nombre" placeholder="Nombre del producto" class="form-control" type="text"
                name="nombre">
        </div>
        <div class="form-group">
            <label for="email">Email</label>
            <input required id="email" placeholder="Email del producto" class="form-control" type="text"
                name="email">
        </div>
        <div class="form-group">
            <label for="genero">Genero</label>
            <input required id="genero" placeholder="Genero del producto" class="form-control" type="text"
                name="genero">
        </div>
        <div class="form-group">
            <label for="contraseña">Contraseña</label>
            <input required id="contraseña" placeholder="Contraseña del producto" class="form-control" type="text"
                name="contraseña">
        </div>
        <div class="form-group">
            <button class="btn btn-success">Guardar</button>
            &nbsp;<a href="/productos" class="btn btn-primary">Volver</a>
        </div>
    </form>
</div>
<%- include("../footer"); %>